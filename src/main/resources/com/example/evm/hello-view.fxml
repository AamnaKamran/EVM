<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="452.0" prefWidth="544.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.evm.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <AnchorPane prefHeight="399.0" prefWidth="504.0">
            <children>
                <Button fx:id="nextBtn" contentDisplay="CENTER" defaultButton="true" layoutX="218.0" layoutY="335.0" onAction="#nextButtonClicked" prefHeight="25.0" prefWidth="68.0" text="Next" textAlignment="CENTER" />
                <Label alignment="CENTER" contentDisplay="TOP" layoutX="137.0" layoutY="110.0" prefHeight="31.0" prefWidth="39.0" text="CNIC:" />
                <TextField fx:id="cnic" layoutX="187.0" layoutY="113.0" prefHeight="25.0" prefWidth="238.0" />
                <Label alignment="CENTER" contentDisplay="TOP" layoutX="134.0" layoutY="142.0" prefHeight="31.0" prefWidth="45.0" text="Name:" />
                <TextField fx:id="name" layoutX="187.0" layoutY="145.0" prefHeight="25.0" prefWidth="238.0" />
                <TextField fx:id="address" layoutX="187.0" layoutY="177.0" prefHeight="51.0" prefWidth="249.0" />
                <Label alignment="CENTER" contentDisplay="TOP" layoutX="61.0" layoutY="173.0" prefHeight="31.0" prefWidth="117.0" text="Permanent Address:" />
                <TextField fx:id="city" layoutX="187.0" layoutY="235.0" prefHeight="25.0" prefWidth="238.0" />
                <Label alignment="CENTER" contentDisplay="TOP" layoutX="142.0" layoutY="232.0" prefHeight="31.0" prefWidth="39.0" text="City:" />
                <Label layoutX="130.0" layoutY="40.0" prefHeight="31.0" prefWidth="245.0" text="ENTER YOUR DETAILS" textAlignment="CENTER" textFill="#365e13" underline="true">
                    <font>
                        <Font name="Calibri" size="28.0" />
                    </font>
                </Label>
            <Label alignment="CENTER" contentDisplay="TOP" layoutX="27.0" layoutY="272.0" prefHeight="31.0" prefWidth="154.0" text="Vote Registration Number:" />
            <TextField fx:id="voteID" layoutX="186.0" layoutY="275.0" prefHeight="25.0" prefWidth="238.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
